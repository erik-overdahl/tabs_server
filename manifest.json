{

  "manifest_version": 2,
  "name": "Tabs Server",
  "version": "0.1",

  "description": "Provide tabs api to external sources",

  "browser_specific_settings": {
    "gecko": {
      "id": "tabs_server@example.org",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/message.svg"
  },

  "permissions": [
    "nativeMessaging",
    "tabs",
    "tabHide",
    "<all_urls>"
  ]
}
